<% layout('layouts/boilerplate') %> 
<h1 class="offset-md-4 my-3"> Listado </h1>
<% const increases = [] %>
<% const decreases = [] %>  
<div class="container rutas-table m-3">
    <div class="row rutas-head">
        <div class="col">
            Artículo
        </div>
        <div class="col">
            Número De Parte
        </div>
        <div class="col">
            Locación
        </div>
        <div class="col">
            Cantidad
        </div>
        <div class="col">
            Entradas
        </div>
        <div class="col">
            Salidas
        </div>
    </div>
    <% inv.forEach((element, index) =>{%>
        <div class="row rutas-row">
            <div class="col">
                <%= element.nombre %>
            </div>
            <div class="col">
                <%= element.partNumber %>
            </div>
            <div class="col">
                <%= element.estante %>
            </div>
            <div class="col">
                <%= element.cantidad %> 
            </div>
            <div class="col">
                <% if(element.increments[index] !== undefined) {%>
                    <% increases.push(parseInt(element.increments[index].qty))%>
                        <%  %> 
                    <%= increases.reduce((a,b) =>{ return a + b}, 0) %>  
                <% }else {%>machupichu<% } %>   
            </div>
            <div class="col">
                <% if(element.decrements[index] !== undefined) {%>
                    <% decreases.push(element.decrements[index].qty) %>
                    <%= decreases.reduce((a,b) => { return a + b}, 0) %>  
                <% } %>  
            </div>
        </div>
    <%})%> 
</div>
<% console.log(increases) %> 