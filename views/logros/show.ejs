<% layout('layouts/boilerplate') %> 
<h1 class="offset-md-4 my-3">Logros de <%= company %> </h1>
<h2 class="offset-md-4 mt-5">Rutas</h2>
<div class="container rutas-table m-5">
    <div class="row rutas-head">
        <div class="col-3">Fecha</div>
        <div class="col-3">Ruta No Realizada</div>
        <div class="col-3">Chofer</div>
        <div class="col-3">Observaciones</div>
    </div>
    <% lists.forEach(i => {%>
        <div class="row rutas-row">
            <div class="col-3">
                <%= i.date %> 
            </div>
            <div class="col-3">
                <%= i.routesNotMade %> 
            </div>
            <div class="col-3">
                <%= i.driver %> 
            </div>
            <div class="col-3">
                <%= i.comments %> 
            </div>
        </div>
   <% }) %> 
</div>